          "format": "table",
          "group": [],
          "rawQuery": true,
          "rawSql": "SELECT\n  zc.zone_name,\n  zc.latitude,\n  zc.longitude,\n  COALESCE(tz.trafic_moyen_vehicules_heure, 0) as traffic_density,\n  zc.zone_type,\n  CASE \n    WHEN tz.trafic_moyen_vehicules_heure > 200 THEN 'Critical'\n    WHEN tz.trafic_moyen_vehicules_heure > 150 THEN 'High'\n    WHEN tz.trafic_moyen_vehicules_heure > 100 THEN 'Medium'\n    WHEN tz.trafic_moyen_vehicules_heure > 50 THEN 'Low'\n    ELSE 'Very Low'\n  END as density_level,\n  CONCAT(\n    zc.zone_name, '\\n',\n    'Traffic: ', ROUND(COALESCE(tz.trafic_moyen_vehicules_heure, 0), 1), ' veh/h\\n',\n    'Level: ', CASE \n      WHEN tz.trafic_moyen_vehicules_heure > 200 THEN 'Critical'\n      WHEN tz.trafic_moyen_vehicules_heure > 150 THEN 'High'\n      WHEN tz.trafic_moyen_vehicules_heure > 100 THEN 'Medium'\n      WHEN tz.trafic_moyen_vehicules_heure > 50 THEN 'Low'\n      ELSE 'Very Low'\n    END\n  ) as tooltip_info\nFROM zone_coordinates zc\nLEFT JOIN trafic_par_zone tz ON zc.zone = tz.zone\nORDER BY zc.latitude, zc.longitude",
          "refId": "A"
        }
